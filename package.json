{
  "name": "money-back",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "migrate": "sequelize-cli db:migrate",
    "shell": "ts-node -T -r tsconfig-paths/register -r ./src/shell.ts",
    "recreateSeeds": "sequelize-cli db:drop --env development && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "pretest": "sequelize-cli db:drop --env test && sequelize-cli db:create --env test && sequelize-cli db:migrate --env test && sequelize-cli db:seed:all --env test",
    "test": "jest --watch -i",
    "coverage": "jest --coverage",
    "dev": "nodemon -q --watch src --ignore 'src/**/*.spec.ts' --exec 'ts-node -r tsconfig-paths/register' src/server.ts | pino-pretty",
    "prod": "pm2 ecosystem.config.js app"
  },
  "dependencies": {
    "@peculiar/webcrypto": "^1.1.1",
    "argon2": "^0.26.2",
    "base64-arraybuffer": "^0.2.0",
    "body-parser": "^1.19.0",
    "bull": "^3.18.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-pino-logger": "^5.0.0",
    "express-ws": "^4.0.0",
    "helmet": "^3.22.0",
    "ioredis": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "nanoid": "^3.1.9",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "pino": "^6.3.2",
    "pm2": "^4.4.0",
    "pug": "^3.0.0",
    "sequelize": "^5",
    "sequelize-cli": "^5.5.1",
    "sparkpost": "^2.1.4",
    "ua-parser-js": "^0.7.21",
    "yup": "^0.29.1"
  },
  "devDependencies": {
    "@types/base64-arraybuffer": "^0.1.0",
    "@types/bluebird": "^3.5.32",
    "@types/body-parser": "^1.19.0",
    "@types/bull": "^3.14.2",
    "@types/cls-hooked": "^4.3.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-ws": "^3.0.0",
    "@types/faker": "^4.1.12",
    "@types/helmet": "^0.0.47",
    "@types/ioredis": "^4.16.4",
    "@types/jest": "^25.2.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.155",
    "@types/multer": "^1.4.3",
    "@types/nanoid": "^2.1.0",
    "@types/node": "^14.0.12",
    "@types/pino": "^6.0.1",
    "@types/pug": "^2.0.4",
    "@types/supertest": "^2.0.9",
    "@types/ua-parser-js": "^0.7.33",
    "@types/validator": "^13.0.0",
    "@types/yup": "^0.29.2",
    "@typescript-eslint/parser": "^3.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "lodash": "^4.17.15",
    "nodemon": "^2.0.4",
    "pino-pretty": "^4.0.0",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": 0,
      "no-unused-vars": 0
    }
  },
  "eslintIgnore": [
    "build/",
    "node_modules/",
    "**/*.spec.ts"
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "setupFiles": [
      "dotenv/config"
    ]
  }
}
