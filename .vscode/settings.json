{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "typescript",
  ],
  "eslint.alwaysShowStatus": true,
  "eslint.packageManager": "yarn",
  "eslint.lintTask.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.background": "#56ab4e",
    "activityBar.activeBorder": "#5058af",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#5058af",
    "activityBarBadge.foreground": "#e7e7e7",
    "titleBar.activeBackground": "#44883e",
    "titleBar.inactiveBackground": "#44883e99",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "statusBar.background": "#44883e",
    "statusBarItem.hoverBackground": "#56ab4e",
    "statusBar.foreground": "#e7e7e7"
  },
  "peacock.color": "#44883e",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
}